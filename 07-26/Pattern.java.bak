/*
  1       2       3 

   6       5       4

   7       8       9

 12      11     10

 13     14     15 
*/

class Pattern 
{
	public static void main(String[] args) 
	{	
		int num=1;
		for(int i=0;i<5;i++) {
			if(i%2==0){
				
				for(int j=0;j<3;j++) {
					System.out.print(num+" ");
					num++;
				}
			} else{
				num+=2;
				for(int j=0;j<3;j++) {
					System.out.print(num+" ");
					num--;
				}
				num+=3;
				
			}
				
			System.out.println("");
		}
			
	}
}
